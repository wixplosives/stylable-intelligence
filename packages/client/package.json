{
    "name": "stylable-intelligence",
    "displayName": "Stylable Intelligence",
    "version": "0.0.54",
    "description": "Code completion, reference detection, diagnostics and other features for Stylable CSS.",
    "author": "Ory Henn (ory@wix.com)",
    "publisher": "wix",
    "icon": "images/logo.png",
    "main": "./dist/src/extension.js",
    "browser": "./dist/src/index.js",
    "scripts": {
        "clear": "rimraf node_modules dist",
        "vscode:prepublish": "rm -rf dist && npm install && tsc -d -p ./tsconfig.publish.json && npm install --prod",
        "build": "tsc -d",
        "compile": "tsc -watch -d",
        "postinstall": "node ./node_modules/vscode/bin/install ",
        "reset": "yarn clear && yarn install && yarn test",
        "pretest": "rimraf dist && yarn build",
        "test": "node ./run-test",
        "test:node": "mocha --reporter mocha-env-reporter dist/test/*.test.js"
    },
    "dependencies": {
        "bignumber": "^1.1.0",
        "css-selector-tokenizer": "^0.7.0",
        "glob": "^7.1.2",
        "lodash": "^4.17.5",
        "postcss": "^6.0.17",
        "stylable": "5.2.0",
        "tslib": "^1.9.0",
        "vscode": "^1.1.10",
        "vscode-languageclient": "^3.5.0",
        "vscode-languageserver": "^3.5.0"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/chai-subset": "^1.3.1",
        "@types/glob": "^5.0.35",
        "@types/lodash": "^4.14.102",
        "@types/mocha": "^2.2.48",
        "@types/node": "8",
        "@types/sinon": "^4.1.3",
        "@types/sinon-chai": "^2.7.29",
        "chai": "^4.1.2",
        "chai-subset": "^1.6.0",
        "concurrently": "^3.5.1",
        "mocha": "^5.0.1",
        "mocha-env-reporter": "^3.0.0",
        "mocha-loader": "^1.1.3",
        "polished": "^1.9.2",
        "rimraf": "^2.6.2",
        "sinon": "^4.3.0",
        "sinon-chai": "^2.14.0",
        "source-map-support": "^0.5.3",
        "typescript": "~2.6.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wix/stylable-intelligence.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wix/stylable-intelligence/issues"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages",
        "Snippets",
        "Other"
    ],
    "contributes": {
        "languages": [
            {
                "id": "stylable",
                "aliases": [
                    "Stylable"
                ],
                "extensions": [
                    ".st.css"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "stylable",
                "scopeName": "source.stylable",
                "path": "./src/stylable.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:stylable"
    ],
    "files": [
        "dist/src"
    ],
    "testGlob": "./dist/test/**/*.spec.js?(x)",
    "homepage": "https://github.com/wix/stylable-intelligence#readme",
    "keywords": [
        "typescript",
        "stylable",
        "css"
    ]
}
